<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
  version="1.0">
  
  <package>org.dcm4chee.xds2.persistence</package>
  
  <access>FIELD</access>

  <entity class="Slot">
    <table name="slot" />
    <sequence-generator name="seq" sequence-name="slot_pk_seq" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value strategy="${generation-type}" generator="seq" />
      </id>
     <basic name="value" optional = "false">
        <column name="value" column-definition="${nvarchar2}"/>
      </basic>
    </attributes>
  </entity>
  <entity class="Identifiable">
    <table name="identifiable" />
    <inheritance strategy="SINGLE_TABLE"/>
    <sequence-generator name="seq" sequence-name="identifiable_pk_seq" />
    <attributes>
      <id name="pk">
        <column name="pk" />
        <generated-value strategy="${generation-type}" generator="seq" />
      </id>
    </attributes>
  </entity>
  
</entity-mappings>
