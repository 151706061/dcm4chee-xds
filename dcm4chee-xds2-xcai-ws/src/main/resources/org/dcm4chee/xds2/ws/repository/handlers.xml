<?xml version="1.0" encoding="UTF-8"?>

<handler-chains xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns1="http://org.jboss.ws/jaxws/handlerscope"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee javaee_web_services_1_2.xsd">

	<handler-chain>
		<protocol-bindings>##SOAP12_HTTP</protocol-bindings>
		<handler>
			<handler-name>ForceMTOMHandler</handler-name>
			<handler-class>org.dcm4chee.xds2.ws.handler.ForceMTOMHandler</handler-class>
		</handler>
		<handler>
			<handler-name>LogHandler</handler-name>
			<handler-class>org.dcm4chee.xds2.ws.handler.LogHandler</handler-class>
		</handler>
	</handler-chain>

</handler-chains>
